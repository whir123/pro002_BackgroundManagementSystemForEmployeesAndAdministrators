{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": [
  "env.d.ts",
  "src/**/*",
  "src/**/*.vue",
  "*.d.ts",
  "src/auto-imports.d.ts",
  "src/components.d.ts"
  ],
  "exclude": ["src/**/__tests__/*"],
  "compilerOptions": {
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",

    "paths": {
      "@/*": ["./src/*"]
    },

    "types": ["vite/client", "element-plus/global"]
  }
}
